{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": {
            "options": ["single", "avoid-escape", "avoid-template"]
        },
        "trailing-comma": {
            "options": [{
                "multiline": "never",
                "singleline": "never"
            }]
        },
        "arrow-parens": false,
        "object-literal-sort-keys": false,
        "typedef": {
            "options": ["call-signature", "parameter", "property-declaration", "member-variable-declaration"]
        }
    },
    "rulesDirectory": []
}