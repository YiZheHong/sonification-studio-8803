{
  "name": "sonification-explorer",
  "version": "0.0.3",
  "description": "Edit and visualize data with charts and sound",
  "private": true,
  "scripts": {
    "test": "jest",
    "clean": "trash dist",
    "lint": "eslint --ext .ts,.js,.vue .",
    "dev": "npm run clean && npm run lint && parcel src/app/public/index.html --open",
    "build": "npm run clean && npm run lint && parcel build src/app/public/index.html --no-source-maps --public-url \"./\"",
    "copy-to-docs": "cross-var \"trash docs/$npm_package_version && copyfiles -f \"./dist/**/*\" ./src/app/public/.nojekyll ./docs/$npm_package_version\"",
    "publish": " npm run build && npm run copy-to-docs"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm test"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/highcharts/sonification-explorer.git"
  },
  "keywords": [
    "sonification",
    "dataviz",
    "chart",
    "data"
  ],
  "author": "Highsoft AS",
  "license": "SEE LICENSE IN LICENSE.md",
  "bugs": {
    "url": "https://github.com/highcharts/sonification-explorer/issues"
  },
  "homepage": "https://github.com/highcharts/sonification-explorer#readme",
  "devDependencies": {
    "@types/jest": "^24.0.23",
    "@typescript-eslint/eslint-plugin": "^2.11.0",
    "@typescript-eslint/parser": "^2.11.0",
    "@vue/component-compiler-utils": "^3.1.0",
    "@vue/test-utils": "^1.0.0-beta.30",
    "babel-core": "^7.0.0-bridge.0",
    "copyfiles": "^2.1.1",
    "cross-var": "^1.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.0.1",
    "husky": "^3.1.0",
    "jest": "^24.9.0",
    "less": "^3.10.3",
    "parcel-bundler": "^1.12.4",
    "sass": "^1.23.7",
    "trash-cli": "^3.0.0",
    "ts-jest": "^24.2.0",
    "typescript": "^3.7.3",
    "vue-jest": "^3.0.5",
    "vue-template-compiler": "^2.6.11"
  },
  "dependencies": {
    "ag-grid-community": "^22.1.1",
    "ag-grid-vue": "^22.1.1",
    "highcharts": "^8.0.0",
    "highcharts-vue": "^1.3.5",
    "vue": "^2.6.11",
    "vue-hot-reload-api": "^2.3.4",
    "vue-property-decorator": "^8.3.0",
    "vuex": "^3.1.2"
  },
  "posthtml": {
    "recognizeSelfClosing": true
  }
}
